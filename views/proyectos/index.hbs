<h2 class="mt-5">
    PRUEBA DE PROYECTOS
</h2>
<div class="proyectos row">
    {{#each proys}}
    <div class="proyecto col-3">
        <h4>{{this.titulo}}</h4>
        <p>{{this.descripcion}}</p>
        <p>
            <a href="{{this.url}}">{{this.url}}</a>
        </p>
        <p>{{this.cliente}}</p>
        <p>{{this.categoria}}</p>
        <a href="/proyectos/{{this._id}}" class="btn btn-block btn-info">Ver detalle</a>
        <a href="/proyectos/edit/{{this._id}}" class="btn- btn-block btn-primary">Editar</a>
    </div>
    {{/each}}
</div>